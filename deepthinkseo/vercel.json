{
  "framework": null,
  "buildCommand": null,
  "outputDirectory": "public",
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "/sitemap.xml"
    },
    {
      "source": "/robots.txt",
      "destination": "/robots.txt"
    },
    {
      "source": "/content-marketing-seo-success-2025",
      "destination": "/blog/content-marketing-seo-success-2025.html"
    },
    {
      "source": "/technical-seo-checklist-2025",
      "destination": "/blog/technical-seo-checklist-2025.html"
    },
    {
      "source": "/complete-seo-strategy-guide-2025",
      "destination": "/blog/complete-seo-strategy-guide-2025.html"
    },
    {
      "source": "/local-seo-strategies-small-businesses-2025",
      "destination": "/blog/local-seo-strategies-small-businesses-2025.html"
    },
    {
      "source": "/ai-powered-seo-strategies-2025",
      "destination": "/blog/ai-powered-seo-strategies-2025.html"
    },
    {
      "source": "/blog",
      "destination": "/blog/index.html"
    },
    {
      "source": "/contact",
      "destination": "/contact.html"
    },
    {
      "source": "/",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
} 