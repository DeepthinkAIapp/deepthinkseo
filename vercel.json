{
  "buildCommand": "cd deepthinkseo && npm run build",
  "outputDirectory": "deepthinkseo/.next",
  "installCommand": "cd deepthinkseo && npm install",
  "rewrites": [
    {
      "source": "/blog/:slug",
      "destination": "/blog/:slug"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
